{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/carta/?$",
			"dest": "_render"
		},
		{
			"src": "^/eventos/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/eventos/?$",
			"dest": "_render"
		},
		{
			"src": "^/eventos/?$",
			"dest": "_render"
		},
		{
			"src": "^/eventos-privados/?$",
			"dest": "_render"
		},
		{
			"src": "^/galeria/?$",
			"dest": "_render"
		},
		{
			"src": "^/nosotros/?$",
			"dest": "_render"
		},
		{
			"src": "^/reservas/?$",
			"dest": "_render"
		},
		{
			"src": "^/usuarios/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}